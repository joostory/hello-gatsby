  },
  "devDependencies": {
    "prettier": "^1.19.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/joostory/hello-gatsby"
  },
  "bugs": {
    "url": "https://github.com/joostory/hello-gatsby/issues"
  }
